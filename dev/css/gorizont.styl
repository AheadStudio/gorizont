.scheme
	position relative

	display none
	width 100%
	margin-top 30px

	transform translateY(50px)
	opacity 0

	transition opacity 0.3s $cubic-transition, transform 0.5s $cubic-transition

	&.active-block
		display block

	&.active
		opacity 1
		transform translateY(0)

	.search
		top -160px
		right 125px
		width 310px

.scheme
	.search
		top -50px
		right 125px
		width 310px

		z-index 10000

	.scheme-information
		position absolute
		left 125px
		top -50px

		display block

		width 270px

		z-index 10000

		.scheme-information-floor-items
			display block
			width 100%

			.scheme-information-floor-item-select
				display block

			.scheme-information-floor-item
				display inline-block
				vertical-align top

				margin-right 10px
				margin-bottom 45px

				padding 20px

				border-radius 30px

				font-size 12px
				font-family GothamProRegular

				background #555555

				color #fff

				transition opacity 0.3s $cubic-transition

				&:hover
					opacity 0.8

				&:last-child
					margin-right 0

				&.active
					background $color-blue

			.jcf-select-scheme-information-floor-item-select
				position relative
				display none !important
				width 120px

				padding 20px
				box-sizing border-box

				border-radius 30px
				background $color-blue

				cursor pointer

				z-index 100000

				.jcf-select-text
					display block

					.jcf-option-scheme-information-floor-item
						position relative
						display block
						box-sizing border-box

						color #fff

						font-size 12px
						font-family GothamProRegular

						z-index 10

				.jcf-select-opener
					position absolute
					top 50%
					right 20px

					width 16px
					height 12px

					transform translateY(-50%)

					&:before
						content "^"
						display block
						width 100%
						height 100%

						font-size 28px
						line-height 1

						color #fff

						transform rotate(-180deg)

						transition transform 0.3s $cubic-transition

				&.jcf-drop-active
					border-bottom-right-radius 0px
					border-bottom-left-radius 0px
					.jcf-select-opener
						&:before
							transform rotate(0deg)

		.scheme-information-quantity
			display block
			width 50%

			.scheme-information-count-title
				display block
				width 100%

				margin-bottom 20px

				font-size 16px

			.scheme-information-count-count
				display block
				width 100%

				font-size 72px

				font-family GothamProMedium

	.scheme-container
		display block
		width 100%

		.scheme-inner-container
			display block
			width 100%
			height 100%
			min-height 100%

			transform rotate3d(-370, -100, -180, 60deg) translate3d(-135px, 5px, -80px)

			.scheme-inner-container-svg
				position relative
				display none

				opacity 0

				transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition

				transform translate3d(30px,-70px, 0)

				z-index -10000

				&.active
					display block

				&.active-scheme-svg
					opacity 1
					transform translate3d(0,0,0)

			.scheme-inner-container-svg-clone
				position absolute
				display block
				width 100%
				max-height 100%
				height auto

				svg

				&.clone-1
					top 2px
					left -1px
					z-index -100
				&.clone-2
					top 5px
					left -2px
					z-index -200
				&.clone-3
					top 6px
					left -3px
					z-index -300
				&.clone-4
					top 8px
					left -4px
					z-index -400
				&.clone-5
					top 10px
					left -5px
					z-index -500
				&.clone-6
					top 12px
					left -6px
					z-index -600
				&.clone-7
					top 14px
					left -7px
					z-index -700
				&.clone-8
					top 16px
					left -7px
					z-index -800

		svg
			position relative
			display block

			width 95%
			max-height 100%
			height auto

			margin-left 2%

			will-change transform

			transition transform 0.3s $cubic-transition


			path
				cursor pointer

				transition fill .3s $cubic-transition

				&:hover
				&.active
					fill rgb(163, 47, 90)


	.scheme-controls
		position absolute
		right 125px
		top 100px

		display block
		width 90px

		.scheme-controls-question
		.scheme-controls-zplus
		.scheme-controls-zminus
			position relative
			display block
			float right

			width 50px
			height 50px

			margin-bottom 15px

			border-radius 50%

			background #555555

			text-align right

			transition opacity 0.3s $cubic-transition

			&:after
				position absolute
				top 50%
				left 50%

				font-size 20px
				line-height 1

				color #fff

				transform translate(-50%,-50%)

			&:hover
				opacity 0.8

		.scheme-controls-question-title
			display block
			width 100%

			text-align right

			font-size 14px

			margin-bottom 50px

		.scheme-controls-question
			&:after
				content "?"
				font-family GothamProLight

		.scheme-controls-zplus
			&:after
				content ""
				display block

				width 14px
				height 1px

				background #fff

			&:before
				content ""
				position absolute
				top 50%
				left 50%

				display block

				width 1px
				height 14px

				background #fff

				transform translate(-50%,-50%)

		.scheme-controls-zminus
			&:after
				content ""
				display block

				width 14px
				height 1px

				background #fff

/* drop jcf */
.jcf-select-drop
	&.jcf-select-scheme-information-floor-item-select
		display block
		.jcf-select-drop-content
			display block
			position relative

			padding 5px 20px 20px 20px

			background #a23f97

			border-bottom-right-radius 30px
			border-bottom-left-radius 30px

			z-index 1000

			.jcf-list-content
				ul
					li
						display block
						padding 10px 0

						font-size 12px

						color #fff
						cursor pointer

