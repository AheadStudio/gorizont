/*** Settings ***/
@import "nib"
@import "../lib/css/css"

global-reset();
reset-html5();
form-reset();
b
strong
i
em
	font-style normal
	font-weight normal


/*-- variables --*/

$cubic-transition = cubic-bezier(0.25, 0.46, 0.45, 0.94)

$color-gray = #969aa3
$color-blue = #a23f97
$color-yellow = #ffd102
$color-darkgray = #333333
$color-red = #ee4784

$color-black-hover =  rgba(#000000, 0.6)
/*-- //variables --*/


/*** //Settings ***/


/*** Fonts ***/
@font-face
	font-family "GothamProBlack"
	src url("../fonts/GothamProBlack.woff") format("woff")

@font-face
	font-family "GothamProBold"
	src url("../fonts/GothamProBold.woff") format("woff")

@font-face
	font-family "GothamProLight"
	src url("../fonts/GothamProLight.woff") format("woff")

@font-face
	font-family "GothamProMedium"
	src url("../fonts/GothamProMedium.woff") format("woff")

@font-face
	font-family "GothamProRegular"
	src url("../fonts/GothamProRegular.woff") format("woff")
/*** //Fonts ***/

/*** structure ***/

html
	height 100%
body
	min-height 100%
	height 100%
	line-height 1

	color #333333
	cursor default

	font-family "GothamProLight", sans-serif

body
	min-height 100%

.page
	width 100%
	min-height 100%

.page-content
	margin 0 auto

.page-content-inner
	transition opacity 0.3s $cubic-transition, transform 0.6s $cubic-transition

	&.loading
		opacity 0
		transform translateY(-50px)
/*** //structure ***/

/*** HEADER STYLES ***/
.page-header
	width 100%
	height 215px

	padding 20px 0 0 0
	box-sizing border-box

	.header-row--top
		margin-bottom 5px

	.header-row
		.header-col
			float left
			box-sizing border-box
			&--1
				width 28%
			&--2
				width 72%

		.header-contacts
			position relative
			display flex
			float left

			width 28%
			padding 17px 0

			z-index 10000

			.header-contacts-phone,
			.header-contacts-location
				width 50%

			.header-contacts-phone
				.header-phone-link
					font-family GothamProMedium

					.header-phone-note
						margin-top 4px

						font-size 12px
						font-weight normal
						font-family GothamProRegular

						text-transform lowercase
						color $color-gray

			.header-contacts-location
				text-align right

				.header-location-point
					position absolute

					top -4px
					height 23px
					width 20px
					left -25px

				.header-location-link
					position relative

					.header-location-link-text
						font-family GothamProMedium

		.header-logo
			text-align center

			.header-logo-img
				position relative
				margin 0 auto

				z-index 10000

/** main menu**/
.header-menu
	position relative
	float left

	width 72%
	display block

	margin-left -15px
	margin-right 15px
	z-index 10000

	.header-menu-inner
		.header-menu-item-holder
			display inline-block

			padding 15px 0px

			border 1px solid transparent
			box-sizing border-box

			font-family GothamProMedium

			.header-menu-item
				padding 5px 5px 5px 3px

				white-space nowrap

				.header-menu-item-name
					font-size 16px

			&--has-submenu
				padding 15px 10px
				width 126px
				margin-right 10px

				.header-menu-item
					padding 5px 0px

				.header-menu-level1
					border 1px solid transparent



		.hovered
			box-sizing border-box

			border 1px solid $color-gray
			border-bottom-color transparent

			.header-menu-level1
				box-sizing border-box

				border-color $color-gray
				border-top-color transparent


				.header-menu-level1-inner
					opacity 1
					filter none
					transform translateY(0)

.header-menu-level1
	position absolute
	left 0
	display none
	box-sizing border-box
	width 126px

	background #fff

	transition background-color 0.5s $cubic-transition
	z-index 10000

	.header-menu-level1-inner
		position relative

		padding 10px
		overflow hidden
		opacity 0

		transform translateY(20px)
		transition transform 0.5s $cubic-transition, opacity 0.5s $cubic-transition


	&-item-holder
		padding 10px 0

		font-family GothamProRegular

		a
			text-decoration none
			color #000

			transition all 0.5s $cubic-transition
			&:hover
				opacity 0.5

.icon-shop
	position relative
	margin-left 30px
	&:before
		content ""
		position absolute
		top 3px
		left -29px

		background-image: url("../svg/icons/shop_white.svg")
		background-size contain
		background-repeat no-repeat

		width 18px
		height 30px

		z-index 100

	&:after
		content ""
		position absolute
		top -2px
		left -38px

		display block

		width 35px
		height 35px

		border-radius 35px

		background $color-blue


/** //main menu **/

/** mobile menu **/


.mobile-header
	position fixed
	top 0
	left 0

	display none
	width 100%

	padding 10px 20px

	margin 0 auto
	box-sizing border-box

	z-index 1000
	background #fff

	.mobile-header-inner
		display inline-block
		width 50%
		vertical-align top

		.mobile-header-logo
			display block
			width 85px
			height 70px

			background-image url("../dummy/logo_mobile.png")
			background-repeat no-repeat

	.mobile-header-controls
		width 48%
		display inline-block
		vertical-align top

		margin-top 25px

		text-align right


		.mobile-header-controls-location
		.mobile-header-controls-mobile
			display inline-block
			width 22px
			height 30px

			vertical-align top

			svg
				display block
				width 100%
				height 100%

		.mobile-header-controls-mobile
			width 14px
			margin-left 23%

		.mobile-header-burger
			position relative
			display inline-block
			width 30px
			height 30px

			margin-left 23%

			.mobile-header-burger-inner
				content ""
				position absolute
				top 50%

				width 30px
				height 3px

				margin-top -2px

				vertical-align top

				background #9c3593

				transition transform 0.3s $cubic-transition

			&:before
				content ""
				position absolute
				top 0
				left 0
				right 0

				width 100%
				height 3px

				background #9c3593
				transition transform 0.3s $cubic-transition, width 0.3s $cubic-transition

			&:after
				content ""
				position absolute
				bottom 0
				left 0
				right 0

				width 100%
				height 3px

				background #9c3593

				transition transform 0.3s $cubic-transition, width 0.3s $cubic-transition

		.mobile-header-burger
			&.active
				.mobile-header-burger-inner
					opacity 0
				&:before
					width 40px
					transform rotate(45deg) translateY(12px) translateX(7px)
				&:after
					width 40px
					transform rotate(-45deg) translateY(-12px) translateX(6px)

.mobile-panel
	position fixed
	top 0
	left 0
	padding 25px 20px

	display block
	width 100%
	height 100%

	box-sizing border-box

	background $color-blue

	z-index 10001

	transform translateX(-100%)
	opacity 0
	overflow-y auto
	transition transform 0.8s $cubic-transition, opacity 0.6s $cubic-transition

	.mobile-panel-section
		display block
		width 100%

		overflow hidden

		margin-bottom 20px

		.mobile-panel-section-item-close
			display block
			width 35px
			height 35px

			border-radius 50%

			transition opacity 0.5s $cubic-transition, transform 0.5s $cubic-transition

			padding 25px
			background #f4f4f4

			float right

			&:focus
				transform rotate(360deg)

			svg
				width 100%
				vertical-align middle


		.mobile-menu
			padding 20px 20px
			border-top 1px solid $color-gray

			.mobile-menu-inner
				display block
				width 100%

				.mobile-menu-item-holder
					font-size 26px

					font-family GothamProMedium

					margin-bottom 40px

					.mobile-menu-item

						&.icon-shop
							margin-left 0

							&:before
								left auto
								right -50px

								background-image url("../svg/icons/shop.svg")

							&:after
								width 40px
								height 40px

								top -5px

								left auto
								right -61px

								background #fff

.show-menu
	.mobile-panel
		opacity 1
		transform translateX(0)


/** //mobile menu **/



/*** //HEADER STYLES ***/


/*** FOOTER STYLES ***/
.page-footer
	width 100%
	height 80px

	margin-top 120px

	background #fff

	.page-inner
		height 100%

	.footer-row
		display flex

		height 100%

		align-items center
		justify-content space-between

		.footer-creators
			float left
			width 32%
			margin-right 2%

			.footer-creators-title
				font-size 14px

			.footer-creators-name
				border-bottom 1px solid #cbcbcb
				transition border-bottom-color 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94)

				font-size 14px
				font-family: GothamProRegular

				text-transform uppercase
			&:hover
				.footer-creators-name
					border-bottom-color transparent

		.footer-copyrights
			width 34%
			font-size 14px


		.footer-social
			width 30%
			margin-left 6%
			text-align right

			.footer-social-title
				font-size 14px
				display inline-block
				float left

				margin-top 8px

			.footer-social-item
				position relative
				display inline-block

				margin 0px 5px

				width 32px
				height 32px

				box-sizing border-box

				border-radius 50%
				border 2px solid black

				transition opacity 0.5s $cubic-transition
				opacity 1

				svg
					position absolute
					width 60%
					display block
					left 50%
					top 50%
					transform: translate(-50%,-50%)
					path
						fill #000000
				&--facebook
					svg
						width 26%
				&--ok
					svg
						width 34%
				&:hover
					opacity: 0.5

/*** //FOOTER STYLES ***/

/*** COMMON STYLES***/

.link
	text-decoration none

.link--bold
	font-weight bold

.link--darkgray
	color #333333

.link--white
	color #fff

.link--black
	color #000


.link--black-bottom
	border-bottom 2px solid transparent
	transition border-bottom-color 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94)
	&:hover
		border-bottom-color #000

.link--black-inverse
	border-bottom 2px solid #000
	transition border-bottom-color 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94)
	&:hover
		border-bottom-color transparent


.link--gray
	color $color-gray

.link--gray-inverse
	border-bottom 1px solid $color-gray
	transition border-bottom-color 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94)
	&:hover
		border-bottom-color transparent


.link--red
	color $color-red

.link--red-bottom
	border-bottom 2px solid transparent
	transition border-bottom-color 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94)
	&:hover
		border-bottom-color $color-red

.link--red-inverse
	border-bottom 2px solid $color-red
	transition border-bottom-color 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94)
	&:hover
		border-bottom-color transparent



.link--blue
	color $color-blue

.link--blue-bottom
	border-bottom 2px solid transparent
	transition border-bottom-color 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94)
	&:hover
		border-bottom-color $color-blue

.link--blue-inverse
	border-bottom 2px solid $color-blue
	transition border-bottom-color 0.5s cubic-bezier(0.25, 0.46, 0.45, 0.94)
	&:hover
		border-bottom-color transparent

.page-max
	display block
	width 100%

	&--relative
		position relative

	&--hidden
		overflow hidden



.page-inner
	width 100%

	max-width 1150px

	margin 0 auto
	box-sizing border-box
	padding 0 20px

	&--w1
		max-width 1150px
	&--w2
		max-width 1366px
		margin 0 auto
	&--relative
		position relative

	&--clearfix
		clearfix()

.page-text
	margin-bottom 45px
	line-height 2
	font-size 16px
	p
		margin-top 45px
		line-height 2
		font-size 16px

	cite
		display block
		width 100%

		margin-bottom 45px
		line-height 50px

		font-size 35px

	.docs
		.docs-item
			position relative
			display inline-block

			margin-bottom 70px

			font-size 34px
			font-family GothamProBold

			line-height 1
			
			color $color-darkgray
			text-decoration none


			.docs-item-text
				margin-left 30px

				border-bottom 1px solid $color-darkgray

				transition border-bottom-color 0.3s $cubic-transition


			.docs-item-icon
				float left

				vertical-align middle

				display inline-block

				width 44px
				height 33px

				padding 6px 5px
				box-sizing border-box
				background $color-blue

				color #fff

				font-size 18px
				font-family GothamProLight

			&:hover
				.docs-item-text
					border-bottom 1px solid transparent



.background-prev
	position relative
	display block
	width 100%
	height 350px
	margin-bottom 60px

	.background-prev-img
		position absolute
		top 0
		left 0

		display block
		width 100%
		height 100%

		object-fit cover


.load-more
	width 100%

	.load-more-btn
		display block

		width 335px
		margin 0 auto

		padding 20px 0

		background #e83b7d

		text-align center

		color #fff
		transition opacity 0.5s $cubic-transition

		font-size 18px

		&:hover
			opacity 0.8


/** search **/

.search
	position absolute

	.search-form
		position relative

	.search-input
		width 100%
		padding 5px 0 14px 0
		border-bottom 1px solid #cccccc

		&::-webkit-input-placeholder
			font-size 14px
			font-family GothamProRegular
			font-weight: 500
			color #333333

			opacity 1

			transition opacity 0.5s $cubic-transition, transform 0.5s $cubic-transition

		&:focus

			&::-webkit-input-placeholder
				opacity 0
				transform translateY(10px)

	.search-submit
		position absolute
		top 0
		right 10px

		width 25px
		height 25px

		&:before
		&:after
			content ""
			position absolute
			top 0
			left 0

			width 100%
			height 100%

			background-repeat no-repeat
			background-size contain

			transition opacity 0.5s $cubic-transition


		&:before
			background-image url("../svg/icons/loupe.svg")

			opacity 1

		&:after
			background-image url("../svg/icons/red_loupe.svg")

			opacity 0

		&:hover
			&:before
				opacity 0
			&:after
				opacity 1




.autocomplete-suggestions
	padding 30px
	box-sizing border-box

	border-bottom 1px solid #cccccc

	background #FFF

	overflow-y auto
	overflow-x hidden

.autocomplete-suggestion
	margin-bottom 30px
	white-space nowrap

	overflow hidden
	border-bottom 1px solid transparent

	transition background 0.6s $cubic-transition, border-bottom-color .3s $cubic-transition

	&:last-child
		margin-bottom 0


	.search-item
		border-bottom transparent

		color $color-darkgray
		text-decoration none

		transition border-bottom 0.6s $cubic-transition

		.search-item-name
			display block
			white-space nowrap

			overflow hidden

			text-overflow ellipsis

			b
				font-family GothamProBold


.autocomplete-selected
	.search-item-name
		border-bottom-color #cccccc


.autocomplete-group
	display none


/** //search **/


/** button up **/

.go-top
	position fixed

	left 50%
	bottom 130px

	display block

	width 20px
	margin-left 510px

	opacity 0
	cursor default

	transition opacity 0.5s $cubic-transition

.go-top.active
	opacity 1
	cursor pointer
	transition opacity 0.5s $cubic-transition

	&:hover
		opacity 0.6


/** //button up **/


/** social block **/

.social-container
	display block

	.social-container-text
		font-size 18px
		float left
		margin-top 6px
		margin-right 20px

	.social-item
		position relative
		display inline-block

		margin 0px 5px

		width 32px
		height 32px

		box-sizing border-box

		border-radius 50%
		border 2px solid black

		transition opacity 0.5s $cubic-transition
		opacity 1

		svg
			position absolute
			width 60%
			display block
			left 50%
			top 50%
			transform: translate(-50%,-50%)

			path
				fill #000000

		&--facebook
			svg
				width 26%
		&--ok
			svg
				width 34%

		&:hover
			opacity: 0.5

/** //social block **/


/** navigation **/

.navigation
	position absolute
	right 35px
	top 10px

	display block

	height 220px

	.navigation-items
		display block
		height 100%


		.navigation-item
			display block
			text-align center

			transition opacity 0.5s $cubic-transition
			&--close
				padding 13px
				margin-bottom 50px
				background #f4f4f4

				border-radius 50%

				svg
					width 13px
					vertical-align middle

			&--prev
				svg
					transform rotate(-90deg)
					width 20px
			&--next
				margin-bottom 45px
				svg
					transform rotate(90deg)
					width 20px
			&:hover
				opacity 0.6

/** //navigation **/

/** button **/

.btn
	display inline-block

	height 65px
	line-height 65px

	box-sizing border-box

	text-align center
	cursor pointer

	transition opacity 0.5s $cubic-transition

	color #000

	font-family GothamProLight

	text-decoration none
	&:hover
		opacity 0.8

.btn--yellow
	background-color $color-yellow

.btn--blue
	background-color $color-blue

.btn--red
	background-color $color-red

/** //button **/


/** acordeon **/

.acordeon-container-items
	overflow hidden

	.acordeon-container-item
		position relative

		display block
		height 150px

		overflow hidden

		background transparent

		border-bottom 1px solid #e6e6e6

		transition height 0.4s $cubic-transition, background 0.2s $cubic-transition

		will-change height

		cursor pointer

		text-decoration none

		img
			position absolute
			top 0
			left 0

			display block

			width 100%
			height 100%

			object-fit cover
			object-position top

			z-index -10
			opacity 0

			transition opacity 0.2s $cubic-transition

		&:first-child
			border-top 1px solid #e6e6e6


		.acordeon-container-item-header
			display flex
			align-items center
			height 150px
			padding 25px 0
			box-sizing border-box

			.acordeon-container-item-header-name
			.acordeon-container-item-header-count
			.acordeon-container-item-header-toggler
				display inline-block
				box-sizing border-box
				color $color-darkgray

				transition color 0.3s $cubic-transition


			.acordeon-container-item-header-name
				width 50%
				text-align left
				font-size 32px

			.acordeon-container-item-header-count
				width 30%
				font-size 32px
				text-align right
				padding-right 65px

			.acordeon-container-item-header-toggler
				width 20%
				text-align right

				.acordeon-container-item-header-toggler-title
					display none
					margin 0 20px
					font-size 18px

					border-bottom 1px solid #c17dba

					opacity 0

					transition opacity 0.3s $cubic-transition, border-bottom-color 0.3s $cubic-transition

					cursor pointer
					&:hover
						border-bottom-color transparent

				.acordeon-container-item-header-toggler-icon
					position relative
					display inline-block

					margin-right 25px
					vertical-align middle
					width 50px
					height 50px

					border 1px solid #000
					border-radius 50%

					transition border-color 0.3s $cubic-transition

					cursor pointer

					&:not(.acordeon-container-item-header-toggler-icon--arrow)
						&:after,
						&:before
							content ""
							position: absolute
							top 50%
							left 50%

							background #000

							transition background 0.3s $cubic-transition

						&:after
							width 20px
							height 1px

							margin -0.5px 0 0 -10px

							transition opacity 0.3s $cubic-transition
						&:before
							width 1px
							height 20px

							margin -10px 0 0 -0.5px

							opacity 1
							transform translateY(0)

							transition opacity 0.3s $cubic-transition, transform 0.5s $cubic-transition

					&.acordeon-container-item-header-toggler-icon--arrow
						svg
							position absolute
							width 16px
							height 26px

							transform rotate(90deg)

							top 50%
							left 50%

							margin-top -13px
							margin-left -8px

							path
								fill #000

								transition fill 0.3s $cubic-transition


		&:hover
			img
				opacity 1

			.acordeon-container-item-header

				.acordeon-container-item-header-name
				.acordeon-container-item-header-count
					color #fff

				.acordeon-container-item-header-toggler-icon

					border-color #fff
					&:not(.acordeon-container-item-header-toggler-icon--arrow)
						&:before
							background #fff
						&:after
							background #fff


					&.acordeon-container-item-header-toggler-icon--arrow
						svg
							path
								fill #fff

				.acordeon-container-item-header-toggler-title
					color #fff


		.acordeon-container-item-content
			position relative

			column-count 4
			column-gap 85px

			display block
			width 100%

			padding 65px 20px 80px 0px

			box-sizing border-box

			&:before
				content ""

				position: absolute
				top 0
				display block

				height 1px

				background #a8a8a8

				width 1000%
				margin-left -500%

				z-index -10

			.acordeon-container-item-content-link
				display block
				position relative

				font-size 18px

				margin-bottom 30px

				&:after
					content ""
					position absolute
					bottom 0

					margin-left 15px

					display inline-block

					width 15px
