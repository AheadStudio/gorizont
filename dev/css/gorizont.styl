/** toltip **/


.tooltip_templates
	display none


.tooltipster-sidetip
	&.tooltipster-white
		.tooltipster-box
			background #fff
			border none
			border-radius 0

.tooltipster-sidetip
	&.tooltipster-white
		.tooltipster-content

			overflow hidden

			.no-info
				padding 20px
				box-sizing border-box
				font-size 16px
				color #000

			.tooltip-close
				position absolute
				right 25px
				top 15px

				display block
				cursor pointer

				span
					line-height 1
					font-size 20px

					color #000

					font-family GothamProRegular


			.tooltip-container
				position relative

				width 200px
				min-height 200px

				padding 30px 20px

				box-sizing border-box

				cursor text

				p
					display block
					width 100%

					margin-bottom 5px

					font-family GothamProLight
					font-size 16px
					color #000

				.tooltip-container-link
					display: block

					padding: 10px 15px
					box-sizing: border-box

					margin-top: 30px

					background $color-red
					color #fff

					transition opacity 0.3s $cubic-transition

					&:hover
						opacity 0.8




	&.tooltipster-top
	&.tooltipster-right
	&.tooltipster-left
	&.tooltipster-bottom
		.tooltipster-arrow
			width 25px
			height 25px

			overflow inherit

			.tooltipster-arrow-uncropped
				&:before
					content ""

					position: absolute
					width 0
					height 0

					border-left 2px solid transparent
					border-right 10px solid transparent
					border-top 20px solid #fff

				.tooltipster-arrow-border
					display none

				.tooltipster-arrow-background
					display none

	&.tooltipster-top
		.tooltipster-arrow
			bottom -15px

	&.tooltipster-bottom
		.tooltipster-arrow
			.tooltipster-arrow-uncropped
				&:before
					transform rotate(-180deg)

	&.tooltipster-right
		.tooltipster-arrow
			.tooltipster-arrow-uncropped
				&:before
					left 2px
					transform rotate(90deg)

	&.tooltipster-left
		.tooltipster-arrow
			.tooltipster-arrow-uncropped
				&:before
					right -8px
					transform rotate(-90deg)
