		left 50%

		display block

		&--floorbasement
			top 2%
			left 41%

			transform translate(-50%,-50px)
			opacity 0

			transition opacity 0.3s $cubic-transition, transform 0.6s $cubic-transition
			transition-delay 0.2s


		&--floor1

			transform translate(-50%,-70px)
			opacity 0

			transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition
			transition-delay 0.4s


		&--floor2
			top 59%
			left 60%

			transform translate(-50%,-80px)
			opacity 0

			transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition
			transition-delay 0.6s


		&--floor3
			top 16%
			left 43%

			transform translate(-50%,-110px)
			opacity 0

			transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition
			transition-delay 0.8s


		&--floor4
			top 52%
			left 58%

			transform translate(-50%,-120px)
			opacity 0

			transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition
			transition-delay 1s


		&--floor5
			top 16%
			left 33.5%

			transform translate(-50%,-130px)
			opacity 0

			transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition
			transition-delay 1.2s


		&--floor6
			top 15%
			left 33%

			transform translate(-50%,-140px)
			opacity 0

			transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition
			transition-delay 1.4s



		.page-preloader-item-holder
			display block

			.page-preloader-item-holder-img
				display block

	&.active-block
		display block
	&.active
		.page-preloader-item
			&--floorbasement
			&--floor1
			&--floor2
			&--floor3
			&--floor4
			&--floor5
			&--floor6
				opacity 1
				transform translate(-50%, 0px)


			.page-preloader-item-holder
				display block

				.page-preloader-item-holder-img
					display block





