/*** popup conventions ***/

.popup-conventions
	position relative

	display block
	width 1040px
	height 340px

	margin 0 auto

.mfp-conventions.mfp-bg
	background-color rgba(255, 255, 255, 1)

.mfp-conventions.mfp-bg
	opacity 0

	transition opacity 0.3s $cubic-transition

	&.mfp-ready
		opacity 0.9

	&.mfp-removing
		opacity 0

.mfp-conventions.mfp-wrap
	.mfp-content
		opacity 0

		transform translateY(-50px)

		transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition

	&.mfp-ready
		.mfp-content
			opacity 1

			transform translateY(0)

	&.mfp-removing
		.mfp-content
			opacity 0

			transform translateY(-50px)

.mfp-conventions
	.popup-conventions
		.mfp-close:not(.mfp-close-icn)
			right -30px
			top -180px

			display block

			width 50px
			height 50px

			border-radius 50%

			background #555555

			opacity 1

			transition opacity 0.3s $cubic-transition, transform 0.3s $cubic-transition

			&:hover
				opacity 0.8
				transform rotate(360deg)

			.mfp-close-icn
				position absolute
				top 50%
				left 50%

				width 10px
				height 10px

				margin 0
				padding 0

				transform translate(-50%,-50%)

				opacity 1


